{
  "parameters": {
    "world_to_cycle": [ "/", "#cycle_length" ],
    "round_cycle": [ "compose", [ "R", "#cycle_length" ] , "#click_time" ],
    "end_cycle": [ "-", "#round_cycle" ]
  },
  "clips": {
    "started": "#identity",
    "stopped": "#identity",
    "starting": [ "trigger_positive", "#stopped", "#end_cycle", "!transition:started" ],
    "stopping": [ "trigger_positive", "#started", "#end_cycle", "!transition:stopped" ]
  },
  "states": [
    "started",
    "stopped",
    "starting",
    "stopping"
  ],
  "transitions": {
    "stopped": "starting",
    "starting": "started",
    "started": "stopping",
    "stopping": "stopped"
  },
  "start_state": "stopped"
}